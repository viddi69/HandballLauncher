import random
import time
 
# Choose a random waiting time in ms in the range from c to d.

def r_wait(c,d) :
    time.time() * random.randint(c,d)


# Randomly choosing an integer in the range from a to b.
def r_int(a,b) :
    random.randint(a,b)




